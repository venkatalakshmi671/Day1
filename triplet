#include<stdio.h>
#include<conio.h>
void main()
{
    int a;
    printf("enter a number:");
    scanf("%d",&a);
    for(int i=5;i<=a;i++)
    {
        for(int j=4;j<i;j++)
        {
            for(int k=3;k<j;k++)
            {
                if(i*i==k*k+j*j)
                {
                    printf("\n%d %d %d",k,j,i);
                }
            }
        }
    }
}
