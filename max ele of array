#include<stdio.h>
void main()
{
    int a[20];
    int l,max;
    printf("Entet length of an array:");
    scanf("%d",&l);
    printf("enter %d no of elementd",l);
    for(int i=0;i<l;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int j=1;j<l;j++)
    {
        if(max<a[j])
        {
            max=a[j];
        }
    }
    printf("maximum is: %d",max);
}
