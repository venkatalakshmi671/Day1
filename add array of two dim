#include<stdio.h>
void addarray(int a1[],int a2[]);
void addarray(int a1[],int a2[])
{
    int i,res[50];
    for(i=0;i<5;i++)
    {
        res[i]=a1[i]+a2[i];
        printf("%d",res[i]);
        }
}
int main()
{
    int array1[4]={1,2,3,4};
    int array2[4]={4,3,3,2};
    addarray(array1,array2);
    return 0;
}
